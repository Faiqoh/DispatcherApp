<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.appdispatcher.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_detail"
        android:name="com.example.appdispatcher.ui.detail.DetailFragment"
        android:label="Detail "
        tools:layout="@layout/fragment_detail_project" />

    <fragment
        android:id="@+id/navigation_payment"
        android:name="com.example.appdispatcher.ui.payment.PaymentFragment"
        android:label="@string/title_payment"
        tools:layout="@layout/fragment_payment" />

    <fragment
        android:id="@+id/navigation_message"
        android:name="com.example.appdispatcher.ui.detail.DetailProgressTaskFragment"
        android:label="@string/title_message"
        tools:layout="@layout/fragment_detail_progress_task" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.appdispatcher.ui.detail.WritingTaskFragment"
        android:label="@string/title_account"
        tools:layout="@layout/writing_task_layout" />
</navigation>